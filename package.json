  "module": "./dist/program.js",
  "typings": "./ts-defs/autobahn-core.d.ts",
  "scripts": {
    "test": "gulp test",
    "update-schemas": "gulp updateSchemas",
    "build": "gulp",
    "ci": "gulp ci",
    "watch:build": "gulp watch",
    "watch": "nodemon --exec npm run watch:build --watch src -e js,mjs,json,feature,ts",
    "example:build": "cd examples/ddg-example && gulp build",
    "example:run": "cd examples/ddg-example && node -r esm ../../dist/cli.js run -p duckduckgo -e qa -h -r xunit -s",
